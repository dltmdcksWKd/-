def solution(s):
    a = 0
    b = 0
    for i in range(len(s)):
        if s[i] == '1':
            a += 1
        else:
            b += 1
    return recursions(a,b)
def recursions(a,b):
    c = 0
    c += 1
    b += b
    if a == 1:
        return c,b
    else:
        return
print(solution("110010101001"))
